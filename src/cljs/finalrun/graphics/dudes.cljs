(ns finalrun.graphics.dudes)

(def dir-map
  {:up [:y -1]
   :down [:y 1]
   :left [:x -1]
   :right [:x 1]})


(defn set-image [dude]
  (let [alt (.floor js/Math (/ (mod  (:tick dude) 20) 10))
        d   (:direction dude)
        img (-> dude :images (get (keyword d)) (nth alt))]
        (assoc dude :img img)))

(defn print-obj-coords [obj]
  (let [{:keys [x y id]} obj]
    (println {:name id :x x :y y})
    obj))

(defn do-movement [dude]
 (let [m (first (:moves dude))]
   (cond (some? m)
			(let [k (first m) v (second m)]
			(-> dude
		 	(update  k + v)
			(update :moves rest)
            set-image))
		 :otherwise dude)))

(defn check-moving [dude]
  (let [m? (and (:moving? dude) (empty? (:moves dude)))
		size (/ (:w dude) 2)
		speed (:speed dude)
		dvec ((:direction dude) dir-map)
		k (dvec 0)
		v (* (dvec 1) speed)]
    	(cond m? (assoc dude :moves (repeat (/ size speed) [k v]))
		  :otherwise dude)))

